{
  "name": "trpg-server",
  "version": "1.2.14",
  "description": "TRPG 标准服务端",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env DEBUG=trpg:* NODE_ENV=development npm run run:nodemon",
    "pro": "cross-env NODE_ENV=production npm run run:standard",
    "run:standard": "node standard.js",
    "run:nodemon": "nodemon standard.js",
    "packages:install": "bash packages/foreach.sh 'npm ci'",
    "prettier:all": "prettier --write \"./packages/**/*.{js,ts}\"",
    "db:migrate:generate": "node ./cli.js generate-migrate",
    "db:migrate:run": "node ./cli.js run-migrate",
    "db:seeder:generate": "node ./cli.js generate-seeder",
    "db:seeder:run": "node ./cli.js run-seeder",
    "docker:dev:build": "docker build . --tag trpg-server:dev",
    "docker:dev:shell": "docker run -it --rm --name=\"trpg-dev\" trpg-server:dev bash",
    "doc:start": "cd ./packages/Website && npm start",
    "playground": "node ./playground/playground.js --experimental-repl-await",
    "bench": "node ./test/bench/index.js && cat ./test/bench/out.log",
    "test": "jest --config=./test/jest.config.js --runInBand",
    "test:noserver": "npm run test --noserver",
    "test:watch": "jest --config=./test/jest.config.js --runInBand --watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./build/husky/commit-msg.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TRPGEngine/Server.git"
  },
  "keywords": [
    "trpg"
  ],
  "author": "moonrailgun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TRPGEngine/Server/issues"
  },
  "homepage": "https://github.com/TRPGEngine/Server#readme",
  "dependencies": {
    "@types/ioredis": "^4.0.18",
    "async-exit-hook": "^2.0.1",
    "axios": "^0.19.0",
    "config": "^3.1.0",
    "crypto-random-string": "^2.0.0",
    "debug": "^4.1.1",
    "execa": "^1.0.0",
    "fs-extra": "^8.0.1",
    "get-port": "^5.0.0",
    "ioredis": "^4.14.1",
    "lodash": "^4.17.15",
    "memoize-one": "^5.0.4",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "nodemon": "^1.19.1",
    "request": "^2.88.0",
    "sequelize": "^5.16.0",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.6.4",
    "uuid": "^3.3.2",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/debug": "^4.1.4",
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.126",
    "@types/memoize-one": "^4.1.1",
    "@types/node-schedule": "^1.2.3",
    "@types/request": "^2.48.3",
    "@types/uuid": "^3.4.4",
    "cross-env": "^5.2.0",
    "glob": "^7.1.4",
    "husky": "^2.4.1",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "sequelize-auto-migrations": "github:TRPGEngine/sequelize-auto-migrations",
    "socket.io-client": "^2.2.0",
    "ts-jest": "^24.0.2"
  }
}
