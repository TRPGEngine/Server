{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "lib": ["es6"],
    "noImplicitAny": false,
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "baseUrl": ".",
  }
}
